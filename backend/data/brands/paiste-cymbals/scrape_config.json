{
  "brand_id": "paiste-cymbals",
  "base_url": "https://www.paiste.com/en/products/cymbals",
  "product_list_selector": ".product-list, .cymbals-grid",
  "product_item_selector": ".product-card, .product-item, article.product",
  "pagination": {
    "type": "button",
    "next_button_selector": ".load-more"
  },
  "fields": {
    "name": {
      "selector": ".product-name, h2",
      "attribute": "text"
    },
    "image_url": {
      "selector": "img",
      "attribute": "src"
    },
    "detail_url": {
      "selector": "a",
      "attribute": "href"
    },
    "price": {
      "selector": ".price",
      "attribute": "text"
    },
    "category": {
      "selector": ".category",
      "attribute": "text"
    }
  }
}