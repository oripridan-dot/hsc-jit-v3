{
  "brand_id": "nord",
  "base_url": "https://www.nordkeyboards.com/products",
  "scraping_strategy": "multi_page",
  "product_pages": [
    "nord-drum-3p",
    "nord-electro-6",
    "nord-grand-2",
    "nord-lead-a1",
    "nord-organ-3",
    "nord-piano-6",
    "nord-stage-4",
    "nord-wave-2"
  ],
  "product_item_selector": "h1",
  "variant_selector": "button[class*=\"Product\"]",
  "consent_selectors": [],
  "post_load_wait_ms": 2000,
  "fields": {
    "name": {
      "selector": "h1",
      "attribute": "text"
    },
    "image_url": {
      "selector": "img[src*=\"product\"], img[src*=\"nord\"], img[alt]",
      "attribute": "src"
    }
  }
}
