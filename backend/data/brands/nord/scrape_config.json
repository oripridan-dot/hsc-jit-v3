{
  "brand_id": "nord",
  "base_url": "https://www.nordkeyboards.com/products/",
  "product_list_selector": ".ProductCardList_ProductCardList__Grid__hppCp",
  "product_item_selector": "a[href^='/products/']",
  "pagination": {
    "type": "none",
    "next_button_selector": null
  },
  "fields": {
    "name": {
      "selector": "h2.sr-only",
      "attribute": "text"
    },
    "image_url": {
      "selector": "img",
      "attribute": "src"
    },
    "detail_url": {
      "selector": "",
      "attribute": "href"
    },
    "price": {
      "selector": null,
      "attribute": "text"
    },
    "category": {
      "selector": null,
      "attribute": "text"
    }
  }
}