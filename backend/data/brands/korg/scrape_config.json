{
  "brand_id": "korg",
  "base_url": "https://www.korg.com/us/products/synthesizers/",
  "product_list_selector": "body",
  "product_item_selector": "section.products_category_block_fix",
  "pagination": {
    "type": "none",
    "next_button_selector": null
  },
  "fields": {
    "name": {
      "selector": "div.products_top_text h3",
      "attribute": "text"
    },
    "image_url": {
      "selector": "div.products_top_image img",
      "attribute": "src"
    },
    "detail_url": {
      "selector": "a",
      "attribute": "href"
    },
    "price": {
      "selector": ".price",
      "attribute": "text"
    },
    "category": {
      "selector": "div.products_top_text h3",
      "attribute": "text"
    }
  }
}